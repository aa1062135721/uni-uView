	    "list": [{
	        "pagePath": "pages/home/index",
	        "iconPath": "static/images/icon_home_noChoose.png",
	        "selectedIconPath": "static/images/icon_home_choose.png",
	        "text": "首页"
	    },
		{
		    "pagePath": "pages/customerService/index",
		    "iconPath": "static/images/icon_kf_nochoose.png",
		    "selectedIconPath": "static/images/icon_kf_choose.png",
		    "text": "客服"
		},
		{
	        "pagePath": "pages/mine/index",
	        "iconPath": "static/images/icon_mine_nochoose.png",
	        "selectedIconPath": "static/images/icon_mine_choose.png",
	        "text": "我的"
	    }]
	}
